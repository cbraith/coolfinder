{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "idiomatic"
  ],
  "rules": {
    "no-console": 0,
    "func-names": 0,
    "indent": 0,
    "space-in-parens": 0,
    "space-before-function-paren": 0,
    "no-unused-vars": 0
  }
}
